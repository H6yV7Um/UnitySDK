
package com.tencent.tauth;


/**
 * UI 错误的封装, {@link IUiListener#onError(UiError)}会使用到
 *
 */
public class UiError {
    /**
     * 错误码
     */
    public int errorCode;
    /**
     * 错误说明
     */
    public String errorMessage;
    /**
     * 错误详细信息
     */
    public String errorDetail;

    /**
     * 
     * @param code
     * @param message
     * @param detail
     */
    public UiError(int code, String message, String detail) {
        errorMessage = message;
        errorCode = code;
        errorDetail = detail;
    }
}
